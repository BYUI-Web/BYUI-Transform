    border-right: 1px solid #e1e1e1;
    border-bottom: 1px solid #e1e1e1;
    padding-top: 10px;
    padding-bottom: 10px;
    padding-left: 50px;
    /* display: none; */
}

.cl-block-content > ul {
    margin: 0px;
}

.cl-icon-housing{
    background-image: url(Images/student-support/checklist/icons/icons-01.png);
}

