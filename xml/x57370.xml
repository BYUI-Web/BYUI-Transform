&lt;script language="JavaScript" type="text/javascript"&gt;
var Tip=new Array() // do not change this!
         
         Tip[0] = "Read your textbook in sections; summarize what you have read after each section.";
         Tip[1] = "Work on difficult and/or unpleasant tasks first.";
         Tip[2] = "After class or textbook reading, record in writing your own words of what you've learned.";
         Tip[3] = "Set academic goals for each of your courses. Having goals will help to keep you focused.";
         Tip[4] = "Use the few minutes between classes to review and preview the lectures.";
         Tip[5] = "Establish consistent sleep and wake schedules, even on weekends.";
         Tip[6] = "Learn the instructor's testing style and study appropriately.";
         Tip[7] = "Prior to class prepare a list of questions to ask in class.";
         Tip[8] = "Pay attention to the charts and pictures in your textbook.";
         Tip[9] = "Sit where you can see the speaker easily and where other distractions are at a minimum.";
         
         var L = Tip.length;
         var whichTip=Math.round(Math.random()*(L-1));
         function showTip(){document.write(Tip[whichTip]);}
         showTip();
&lt;/script&gt;
&lt;br /&gt; &lt;a href="/academic-support-centers/study-skills/study-tips" id="info"&gt;[more tips]&lt;/a&gt;&lt;/p&gt;
