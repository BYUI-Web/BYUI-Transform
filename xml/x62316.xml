<ContentSnippet ID="x62316" Name="Top Nav" IsComponent="true"><ContentSnippet type="string" label="Content Snippet" readonly="false" hidden="false" required="false">&lt;style&gt;
ul#top-nav {
	width: 100%;
	float: left;
	margin: 0 0 25px 0;
	padding: 0;
}
ul#top-nav &gt; li,
li.hidden-nav-item
{
	list-style-type: none;
	background-color: #0f6cb6;
	margin: 0;
	padding: 0;
}
ul#top-nav &gt; li {
	float: left;
	width: 25%;
}
ul#top-nav &gt; li a {
	text-align: center;
}
ul#top-nav &gt; li a,
li.hidden-nav-item &gt; a
{
	color: #fff;
	padding: 5px 0;
	text-decoration: none;
	font-size: 0.9em;
	font-weight: bold;
	font-family: "Century Gothic", "Helvetica Neue", Helvetica, Arial, sans-serif;
	letter-spacing: 1.5pt;
	display: block;
}
ul#top-nav &gt; li &gt; a:hover,
ul.hidden-nav-menu &gt; li &gt; a:hover
{
	background-color: #000;
}
ul.hidden-nav-menu {
	display: none;
}
li:hover &gt; ul.hidden-nav-menu {
	display: block;
}
ul#top-nav &gt; li:nth-child(1n+2) &gt; a,
ul.hidden-nav-menu
{
	border-left: 1px solid #fff;
}
ul#top-nav a,
ul.hidden-nav-menu
{
	
}
&lt;/style&gt;

