<script type="text/javascript">
window.onload=function(){
$(document).ready(function() {
     var cssObj = {
        'background-image' : "url('Images/student-support/Get Involved/get-involved-sprite.jpg')",
        'background-position' : '0 4321px'
    };
    $('#center_stage').css(cssObj);
    
    
    function slide(nav, backgroundPosition, li, side_position){
        $(nav).on('mouseenter mouseleave', function(){
        $(li).stop(false,true).slideToggle();
        var center_stageObj = {
            'background-image' : "url('Images/student-support/Get Involved/get-involved-sprite.jpg')",
            'background-position' : '0 '+backgroundPosition
        },
            side_Obj = {
                'background-image' : "url('Images/student-support/Get Involved/get-involved-side-sprite.jpg')",
                'background-position' : side_position+' 0'
            };
        $('#center_stage').css(center_stageObj);
        
        $('#get_involved').css(side_Obj);
        
    });
    }//end slide function
    
    function main_picture_slide(backgroundPosition){
        var center_stageObj = {
            'background-image' : "url('Images/student-support/Get Involved/get-involved-sprite.jpg')",
            'background-position' : '0 '+backgroundPosition
        };
        
        $('#center_stage').css(center_stageObj);
        
    }

    slide('.dropdown_one', '3081px', '.dropdown_one li','194px');
    slide('.dropdown_two',  '3700px','.dropdown_two li','389px');
    slide('.dropdown_three',  '2462px','.dropdown_three li','584px');
    
    $('nav li').hover(function(){
        var backgroundPosition = $(this).data('picture');
        main_picture_slide(backgroundPosition);
        
    });
    
    $('nav li').on('click', function(){
        var class_name = $(this).attr('class');
        switch(class_name){
            case "li_one":
                window.location='http://www.byui.edu/activities';
                break;
            case "li_two":
                window.location='http://www.byuiconnect.com/s/1085/07-idaho/idaho-start.aspx?gid=4&pgid=61';
                break;
            case "li_three":
                window.location='http://www.byui.edu/student-support';
                break;
            case "li_two_one":
                window.location='http://www.byui.edu/center-stage';
                break;
            case "li_two_three":
                window.location='http://www.byui.edu/online/pathway';
                break;
            case "li_two_four":
                window.location='http://www.byui.edu/online';
                break;
            case "li_two_five":
                window.location='http://www2.byui.edu/StudentHonor/Student%20Honor%20Class%20Talks.htm';
                break;
            case "li_two_six":
                window.location='http://www.byui.edu/university-operations/security-and-safety/student-safety-council';
                break;
            case "li_two_seven":
                window.location='http://byui.universitytickets.com/user_pages/event_listings.aspx?id=110';
                break;
            case "li_three_one":
                window.location='http://www.byui.edu/societies';
                break;
            case "li_three_two":
                window.location='http://www.byui.edu/teacher-education/accreditation-2011/professional-certification-programs/secondary-education/performing-arts-grouping';
                break;
            case "li_three_three":
                window.location='http://www.byuicomm.net/tag/rotc/';
                break;
        }
    });
    

}); // end ready
}
</script>
