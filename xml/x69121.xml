}
ul.pagination {
   width:150px; height: 12px; position: relative; overflow:hidden; MARGIN-top: 15px; 
}
ul.pagination li {
   float:left; margin:0 1px; list-style:none;      
}
ul.pagination li a {
   display:block; width:12px; height:0; padding-top:12px; background-image:url(http://courses.byui.edu/AcademicSupport/pics/pagination.png); background-position:0 0; float:left; overflow:hidden;
}
.pagination li.current a {
   background-position:0 -12px;
