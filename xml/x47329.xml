            $('.major').click(function() {
                var selected = $(this).attr('id');
                if ($(this).attr('class') == 'major active_major') {
                    $(this).removeClass('active_major');
                    i--;
                    console.log('i <= 5 && has .active_major');
                    console.log('-' + selected);
                    $('.more_help' + '.' + selected).fadeOut(100);
                } else {
                    if (i <= 5) {
                        $(this).addClass('active_major');
                        i++;
                        console.log('i <= 5 != .active_major');
                        console.log(selected);
                        $('.more_help' + '.' + selected).fadeIn(500);
                    }
                }
            });
        });
