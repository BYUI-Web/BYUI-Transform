/* STYLING HERE */
	
	/* COLORS 
	salmon = #F06030
	green = #D6DE23
	*/

	.templates_box {
		background-color: #F06030;
		margin-bottom: 10px;
		overflow: hidden;
		color: #fff;
		padding: 20px 0;
	}
	.templates_box .col {
		padding: 0 20px;
	}
	.templates_box h2 { text-align: center; color: #fff; font-size: 16pt; padding-bottom: 20px; }
	
	.templates_box p { color: #fff; font-family: helvetica, sans-serif; margin-bottom: 16px; }
	
	.templates_box li { color: #fff; font-family: helvetica, sans-serif; font-size: 12px; }
	
	.templates_box span.small { font-size: 11px; font-family: helvetica, sans-serif; padding: 0; margin: 0; }
	
	.template_thumbnail {
		border-bottom: 1px solid #fff;
		padding-bottom: 20px;
		margin-bottom: 20px;
	}
	
	.template_thumbnail a {
		display: block;
		width: 190px;
		height: 190px;
		margin: 0 auto;
		background: url(images/university-communications/templates-sprite.png);
		border-radius: 5px;
		text-indent: -99999px;
	}
	.template_thumbnail a#department-logos {background-position: -380px 0px;}
	.template_thumbnail a#ad-templates {background-position: -190px 0px;}
	
	.template_thumbnail a#department-logos:hover {background-position: -380px -190px;}
	.template_thumbnail a#ad-templates:hover {background-position: -190px -190px;}
	.template_thumbnail a#office-templates:hover {background-position: 0px -190px;}
	
	.vline { 
		border-right: 1px solid #fff; 
		height: 100%;
		width: 0;
		position: absolute;
		top: 0;
		right: -6px;
	}
	span.small { font-size: 12px; padding: 0; margin: 0; }
	.formats {
		overflow: hidden;
	}
	.formats img { float: left; }
	
	.marketing_box, .publicity_box {
		background-color: #D6DE23;
		position: relative;
		margin-bottom: 10px;
	}
	
	.marketing_box p, .publicity_box p {
		color: #000;
		font-family: helvetica, sans-serif;
		font-size: 11px;
		text-align: center;
		margin: 0;
		padding: 0;
	}
	
	.resources_thumbnail {
		float: left;
		padding: 20px 39px;
		position: relative;
	}
	
	.resources_thumbnail a {
		display: block;
		width: 77px;
		height: 77px;
		margin: 0 auto;
		border-radius:5px;
		margin: 10px;
		text-indent: -99999px;
		background: url(images/university-communications/resources-sprite.png);
	}
	
	.resources_thumbnail a#mktg-options-link {background-position: -231px 0px;}
	.resources_thumbnail a#mkg-tips-link {background-position: -154px 0px;}
	.resources_thumbnail a#media-link {background-position: -77px 0px;}
	
	.resources_thumbnail a#mktg-options-link:hover {background-position: -231px -77px;}
	.resources_thumbnail a#mkg-tips-link:hover {background-position: -154px -77px;}
	.resources_thumbnail a#media-link:hover {background-position: -77px -77px;}
	.resources_thumbnail a#contact-link:hover {background-position: 0px -77px;}
	
	.resources_thumbnail .vline {
		height: 112px;
		top: 20px;
	}
--></style>
