<Content_wSideColumn ID="x32036" Name="Eating Disorders Self Test" Layout="general_content.xsl"><Title type="string" label="Title" readonly="false" hidden="false" required="true">Self-Test for Eating Disorders</Title><Abstract type="string" label="Abstract" readonly="false" hidden="false" required="true">Self-Test for Eating Disorders from the BYU-Idaho Counseling Center</Abstract><xPower Type="GroupStart" GroupStart="" Name="SideColumn" label="L/R Column" readonly="false" hidden="false" required="false" Expanded="false"><MoveLeft type="boolean" label="Toggle column right or left (right is default)" readonly="false" hidden="false" required="false">false</MoveLeft><xPower Type="Component" Component="" Name="ImageComponent" label="Image Component" readonly="false" hidden="false" required="false" CompTypes="LeftColumnImageComponent"/><xPower Type="Component" Component="" Name="TextComponent" label="Text Component" readonly="false" hidden="false" required="false" CompTypes="ColumnTextComponent"/><fbookTop type="boolean" label="Put facebook at top of page" readonly="false" hidden="false" required="false">false</fbookTop><fbookStatus type="string" label="Facebook ID#" readonly="false" hidden="false" required="false"></fbookStatus><fbookToken type="string" label="Access_Token" readonly="false" hidden="false" required="false"></fbookToken><fbookDisplayed type="string" label="Number of facebook messages to display" readonly="false" hidden="false" required="false">1</fbookDisplayed></xPower><BodyCopy type="dhtml" label="Body Copy" readonly="false" hidden="false" required="false"><![CDATA[<script type="text/Javascript">
function radio(question, num, numAnswers) {
for (var i = 1; i < numAnswers + 1; i++) {
var img = document.getElementById(question * 10 + i);
img.src = "http://ingeniux.byui.edu/beta/xml/Images/counseling-center/radio.png";
}

var img = document.getElementById(question * 10 + num);
img.src = "http://ingeniux.byui.edu/beta/xml/Images/counseling-center/radio2.png"; 
}

function getResult() {
	var score = 0;

	for (var i = 1; i < 21; i++) {
		var max;

		if (i == 9) {

			max = 5;

		} else if (i == 4 || i == 6 || i == 13) {

			max = 2;

		} else {

			max = 4;

		}

                var temp = 0;
		for (var j = 1; j < max + 1; j++) {
			var button = document.getElementById(i * 10 + j);
                        
			if (button.src == "http://ingeniux.byui.edu/beta/xml/Images/counseling-center/radio2.png") {
                                temp = j;
				break;
			}

		}
                if (temp == 0) {
                        score = 0;
                        break;
                } else {
                        score += temp;
                }
	}

	var output = document.getElementById("output");

        if (score < 20) {
                output.innerHTML = "<h4>You have to answer all of the questions.</h4>"
	} else if (score < 30) {
		output.innerHTML = "<h4>You have strong tendencies towards anorexia nervosa. It would be wise for you to seek out some kind of help or assistance.</h4>";
	} else if (score < 45) {
		output.innerHTML = "<h4>You have strong tendencies towards bulimia. It would be wise for you to seek out some kind of help or assistance.</h4>";
	} else if (score <= 55) {
		output.innerHTML = "<h4>Weight conscious, not necessarily with anorexic or bulimic tendencies.</h4>";
	} else {
		output.innerHTML = "<h4>No need for concern.</h4>";
	}
}
</script>
