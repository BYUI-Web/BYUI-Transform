&lt;script&gt;
$(document).ready(function(){

var bannerOne = "Images/housing/Homepage/banner1.png";
var bannerTwo = "Images/housing/Homepage/banner2.png";
var bannerThree = "Images/housing/Homepage/StudentLivingApartmentBanner.png";

banner(bannerOne,bannerTwo,bannerThree,"banners", "x44817.xml");

	function banner(imageOne, imageTwo, imageThree, divID, link) {
		var container = document.getElementById (divID);
		
		var imageOneElement = document.createElement("img");
		imageOneElement.src = imageOne;
		imageOneElement.classList.add("imageOne");
		
		var imageTwoElement = document.createElement("img");
		imageTwoElement.src = imageTwo;	
		imageTwoElement.classList.add("imageTwo");
		
		var imageThreeElement = document.createElement("img");
		imageThreeElement.src = imageThree;
		imageThreeElement.classList.add("imageThree");
		
		imageOneElement.style.position = "absolute";
		imageTwoElement.style.position = "absolute";
		imageThreeElement.style.position = "absolute";
		imageThreeElement.style.display = "none";
		
		var imageOneLink = document.createElement("a");
		imageOneLink.href = link;
		imageOneLink.appendChild(imageOneElement);		
		var imageTwoLink = document.createElement("a");
		imageTwoLink.href = link;
		imageTwoLink.appendChild(imageTwoElement);		
		var imageThreeLink = document.createElement("a");
		imageThreeLink.href = link;
		imageThreeLink.appendChild(imageThreeElement);
		
		container.appendChild(imageTwoLink);
		container.appendChild(imageOneLink);
		container.appendChild(imageThreeLink);
	
		$( imageOneElement &amp;&amp; imageTwoElement &amp;&amp; imageThreeElement ).load(function() {
			setTimeout(
  				function(){
  					$(".imageOne").fadeOut(2000);
  					$(".imageTwo").fadeOut(6000);
  					setTimeout(function(){$(".imageThree").fadeIn(4000)},3000);
  				}, 2000);
		});
	}

});
&lt;/script&gt;


