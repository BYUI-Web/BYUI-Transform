(function ($) {
    function errorLog(message){
        var dat = new Date();
        console.log(dat.getMilliseconds() + " :: ERROR :: " + message);
    }

    function getParent(obj){
        var bottomNav = "#cat-bottom";
        var topNav = "#cat-top";
        if(obj.parents(topNav).length != 0){
            return topNav;
        } else if(obj.parents(bottomNav).length != 0){
            return bottomNav 
        }
        
        errorLog("No Parents found in getParent");
        return null;
    }

    function showInfoForElem(elem, prnt){
        var prevActLi = $(".cat-nav li.active");
        $(".cat-nav li").toggleClass("active", false);
        var prevActive = $(".cat-info-block:visible");
        var prevParent = getParent(prevActive);
        $(".cat-info-block").hide();
        var liElem = elem.parents("li");
        var index = liElem.prevAll(".nav-item").length;
        var infoElem = $($(prnt).find(".cat-info-block")[index]);
        liElem.toggleClass("active", true);

        if(window.PIE && $('html').is(".ie6,.ie7")){
                if(prevActLi.length > 0){
                     window.PIE.detach(prevActLi[0]); 
                }
                window.PIE.attach(liElem[0]);
        }
        //only slide if it panal changes
        if(prevParent != null && prevParent == prnt){
            infoElem.show();
        }
        else{
            infoElem.slideDown(100);
        }

    }

    $(document).ready(function(){
        var bxSliderURL = "Documents/student-support/Get%20Involved/jquery.bxslider.min.js";
            var slider = $(".bxslider").bxSlider({
                pager: false,
                controls: false,
                auto: true,
                autoHover: true,
                onSlideBefore: fadeAndShowNextCaption
            });
            $("#sl-left-arrow").click(function(){
                fadeAndShowNextCaption();
                slider.goToPrevSlide();
            });

            $("#sl-right-arrow").click(function(){
                fadeAndShowNextCaption();
                slider.goToNextSlide();
            });

        $(".cat-nav a").click(function(evt){
            evt.preventDefault();
            var elem = $(evt.currentTarget);
            var realParent = getParent(elem);

            //get the index of the element clicked
            showInfoForElem(elem, realParent);
            //return false;
        });    

        
        function fadeAndShowNextCaption(){
            var curActive = $(".sl-msg.active");
            var toBe = curActive.next(".sl-msg");

            if(toBe.length == 0){
                toBe = $(".sl-msg").first();
            }

            curActive.fadeOut('fast', function(){
                    curActive.toggleClass('active', false);
                    curActive.toggleClass('jsinline-block', false);
                    toBe.fadeIn('fast', function(){
                        toBe.toggleClass('active', true);
                    }).toggleClass('jsinline-block', true);
            });
        }
    });
}(mJquery));
