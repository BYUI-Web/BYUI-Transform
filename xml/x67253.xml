			if (window.XMLHttpRequest)
			  {
			  xhttp=new XMLHttpRequest();
			  }
			else // Internet Explorer 5/6
			  {
			  xhttp=new ActiveXObject("Microsoft.XMLHTTP");
			  }
			xhttp.open("GET","xml/kbyrfeed.xml",false);
			xhttp.send("");
			xmlDoc=xhttp.responseXML; 
			
			
			var x=xmlDoc.getElementsByTagName("audio");
			for (i=0;i<1;i++) //changed due to broadcasting laws, used to be: for (i=0;i<x.length;i++)
			  { 
			  
			  if (x[i].getElementsByTagName("status")[0].childNodes[0].nodeValue == 'Playing')
			  {
			  	document.write("<div class='sched_item_playing'>");
			  }
			  else 
			  {
			  	document.write("<div class='sched_item'>");
			  }

			  if (x[i].getElementsByTagName("played_date")[0].hasChildNodes())
			  {
				  document.write("<div class='sched_left_col'><div class='schedule_date_time'>");
				  document.write(x[i].getElementsByTagName("played_date")[0].childNodes[0].nodeValue);
				  document.write(" - ");
			  }
			  if (x[i].getElementsByTagName("played_time")[0].hasChildNodes())
			  {
				  document.write(x[i].getElementsByTagName("played_time")[0].childNodes[0].nodeValue);
				 // document.write("  (24HR)<br />");
				  document.write("</div></div>");
			  }
			  if (x[i].getElementsByTagName("type")[0].hasChildNodes())
			  {
				  document.write('<div class="sched_right_col"><div class="schedule_type">');
				  /* document.write(x[i].getElementsByTagName("type")[0].childNodes[0].nodeValue); */
				  document.write("</div>");
			  }
			  if (x[i].getElementsByTagName("artist")[0].hasChildNodes())
			  {
				  document.write('<div class="schedule_artist">');
				  document.write(x[i].getElementsByTagName("artist")[0].childNodes[0].nodeValue);
				  document.write("</div>");
			  }

			  if (x[i].getElementsByTagName("title")[0].hasChildNodes())
			  {
				  document.write('<div class="schedule_title">');
				  document.write(x[i].getElementsByTagName("title")[0].childNodes[0].nodeValue);
				  document.write("</div>");
			  }
			  					  if (x[i].getElementsByTagName("comment1")[0].hasChildNodes())
			  {
				  document.write('<div class="schedule_comment1">Additional Info: <br /> ');
				  document.write(x[i].getElementsByTagName("comment1")[0].childNodes[0].nodeValue);
				  document.write("</div>");
			 }
			  //if (x[i].getElementsByTagName("composer")[0].hasChildNodes())
