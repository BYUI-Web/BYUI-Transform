         if(k.search(new RegExp(/beta_/i))===0){
            if(!(k.split('_')[2] in ilearn.betaTags)){
               ilearn.betaTags[k.split('_')[2]] = {};
            }
            if(k.indexOf('_invite')!=-1){
               ilearn.betaTags[k.split('_')[2]].invite = v;
            }
            else if(k.indexOf('_reminder')!=-1){
               ilearn.betaTags[k.split('_')[2]].reminder = v;
            }
            else if(k.indexOf('_name')!=-1){
               ilearn.betaTags[k.split('_')[2]].title = v;
            }
            else{
               ilearn.betaTags[k.split('_')[2]].name = k;
               ilearn.betaTags[k.split('_')[2]].active = v;
               ilearn.betaTags[k.split('_')[2]].location = k.split('_')[2];
               ilearn.betaTags[k.split('_')[2]].public = false;
               for (pub in ilearn.publicKeys){
                  if (ilearn.publicKeys[pub].location == k.split('_')[2]){
                     ilearn.betaTags[k.split('_')[2]].public = true;
                  }
               }
            }
         }
         if(k.search(new RegExp(/calColor_/i))===0){
            if(!(k.split('_')[1] in ilearn.calPrefs)){
               ilearn.calPrefs[k.split('_')[1]] = {};
            }
            if(k.indexOf('_color')!=-1){
               ilearn.calPrefs[k.split('_')[1]].color = v;
            }
            else if(k.indexOf('_border')!=-1){
               ilearn.calPrefs[k.split('_')[1]].border = v;
            }
         }
         if(k.search(new RegExp(/lastCalView/i))===0){
            if(!('lastView' in ilearn.calPrefs)){
               ilearn.calLastView = v;
            }
         }
      });
   }
});
