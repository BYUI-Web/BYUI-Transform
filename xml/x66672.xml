<script type="text/javascript">
window.onload = function(){
    (function(){
        var content = $('.content'),
            top = $('.top'),
            checklist = {
            init: function(){
                content.hide();
                top.on('click', checklist.show);
                
            },
            show: function(){
                var arrow = $(this).find('.head').find('div.arrow');
                    

                $(this).next('.content').slideToggle();
                arrow.toggleClass('up_arrow');
                arrow.toggleClass('down_arrow');


            },
            read: function(){
                var arrow = $(this).find('.head').find('div.arrow');

                $('<p class="read_more">Read More</p>')
                    .appendTo('.top .head');

            }
        };
        checklist.init();

    })();
}
</script>
