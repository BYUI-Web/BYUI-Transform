<Content_wSideColumn ID="x62492" Name="I-Card" Layout="byuidefault.xsl"><Title type="string" label="Title" readonly="false" hidden="false" required="true">I-Card</Title><Abstract type="string" label="Abstract" readonly="false" hidden="false" required="true">I-Card</Abstract><xPower Type="GroupStart" GroupStart="" Name="SideColumn" label="L/R Column" readonly="false" hidden="false" required="false" Expanded="false"><MoveLeft type="boolean" label="Toggle column right or left (right is default)" readonly="false" hidden="false" required="false">false</MoveLeft><xPower Type="Component" Component="" Name="ImageComponent" label="Image Component" readonly="false" hidden="false" required="false" CompTypes="LeftColumnImageComponent"/><xPower Type="Component" Component="" Name="TextComponent" label="Text Component" readonly="false" hidden="false" required="false" CompTypes="ColumnTextComponent"/><fbookTop type="boolean" label="Put facebook at top of page" readonly="false" hidden="false" required="false">false</fbookTop><fbookStatus type="string" label="Facebook ID#" readonly="false" hidden="false" required="false"></fbookStatus><fbookToken type="string" label="Access_Token" readonly="false" hidden="false" required="false"></fbookToken><fbookDisplayed type="string" label="Number of facebook messages to display" readonly="false" hidden="false" required="false">1</fbookDisplayed></xPower><BodyCopy type="dhtml" label="Body Copy" readonly="false" hidden="false" required="false"><![CDATA[<p style="font-family: Verdana, Arial, sans-serif;">The BYU-Idaho ID Card (called I-Card) is your key to campus life. I-Cards are issued to full- and part-time students, as well as faculty, staff and dependents. To obtain an I-Card, bring a photo ID and your I-Number to the ID Center. Use the links below to learn more about the I-Card and services available to card holders.</p>
<dl style="font-family: Verdana, Arial, sans-serif;"><dt><a href="x32772.xml" title="Location and Hours"><strong>Location and Hours</strong></a></dt><dd style="padding-left: 10px;">Where and when do you get your I-Card? Find out here!</dd><br /><dt><a href="x34007.xml" title="Lost I-Cards"><strong>Lost I-Cards</strong></a></dt><dd style="padding-left: 10px;">Lost your I-Card? Find out how to get a new one!</dd><br /><dt><a href="x34006.xml" title="I-Card Policies"><strong>I-Card Policies</strong></a></dt><dd style="padding-left: 10px;">Important policies and procedures regarding the use and care of the I-Card</dd><br /><dt><a href="x25575.xml" title="Online Student I-Card Application Form"><strong>Online Student I-Card Application Form</strong></a></dt><dd style="padding-left: 10px;">If you're an online student and you need an I-Card, send this form in and you'll receive yours by mail.</dd><br /><dt><a href="x35093.xml#accounting_reporting" title="Who to Call"><strong>Who to Call</strong></a></dt><dd style="padding-left: 10px;">Still have questions? Here's a list of people who can help!</dd></dl>]]></BodyCopy><xPower Type="Component" Component="" Name="HighlightedLinks" label="Highlighted Links" readonly="false" hidden="false" required="false" CompTypes="HighlightedLinksComponent" Expanded="false"/><xPower Type="GroupStart" GroupStart="" Name="SEOGroup" label="SEO Group" readonly="false" hidden="false" required="false" Expanded="false"><BrowserBarTitle type="string" label="Browser Bar Title" readonly="false" hidden="false" required="false"></BrowserBarTitle><MetaDescription type="string" label="Meta Description" readonly="false" hidden="false" required="false"></MetaDescription><NoFollow type="boolean" hidden="true" label="No Follow" readonly="false" required="false">false</NoFollow><NoIndex type="boolean" hidden="true" label="No Index" readonly="false" required="false">false</NoIndex><NoODP type="boolean" hidden="true" label="No ODP" readonly="false" required="fals