<script type="text/javascript">
$('.cl-block-content').css("display", "none");
    window.onload = function() {
      (function() {
        var content = $('.cl-block-content'),
          top = $('.cl-block-top'),
          checklist = {
            init: function() {
              top.on('click', checklist.show);
              content.css("display", "none");
            },
            show: function() {
              var arrow = $(this).find(".cl-arrow");
              var contentBlock = $(this).next(content);
              var isVisible = contentBlock.is(":visible");

              contentBlock.slideToggle();
              //if content is visible display the up arrow
              arrow.toggleClass('cl-up-arrow', !isVisible);
              //otherwise display the down arrow
              arrow.toggleClass('cl-down-arrow', isVisible);
            }
          };
        checklist.init();

      })();
    };
</script>
