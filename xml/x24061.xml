nk="" Expanded="true"><EducationItem readonly="false" hidden="false" required="false" LinkText="" Target="" Link="" Expanded="true"></EducationItem></xPower><xPower Type="List" List="InterestItem" Name="InterestItems" label="Areas of Interest" readonly="false" hidden="false" required="false" LinkText="" Target="" Link="" Expanded="true"><InterestItem readonly="false" hidden="false" required="false" LinkText="" Target="" Link="" Expanded="true"></InterestItem></xPower><xPower Type="List" List="xpowerlink_CurrentCourseItem" Name="CurrentCourseItems" label="Current Courses Taught" readonly="false" hidden="false" required="false" LinkText="" Target="" Link="" Expanded="true"><xPower Type="Link" Link="" Name="CurrentCourseItem" readonly="false" hidden="false" required="false" LinkText="" Target="" Expanded="true"/></xPower><xPower Type="List" List="xpowerlink_OtherCourseItem" Name="OtherCourseItems" label="Other Courses Taught" readonly="false" hidden="false" required="false" LinkText="" Target="" Link="" Expanded="true"><xPower Type="Link" Link="" Name="OtherCourseItem" readonly="false" hidden="false" required="false" LinkText="" Target="" Expanded="true"/></xPower></xPower><xPower Type="GroupStart" GroupStart="" Name="OptionalInfo" label="Optional Info Items" readonly="false" hidden="false" required="false" List="xpowergroupstart_OptionalInfo" LinkText="" Target="" Link="" Expanded="true"><OptionalItemsText1 type="dhtml" label="Optional Item Text 1" readonly="false" hidden="false" required="false"><![CDATA[<p style="text-align: left;"><span style="font-family: andale mono,times; font-size: small;">Dr. Colton D. Miller earned his Doctorate degree in Counseling Psychology from Brigham Young University after graduating from BYU-Idaho in 2006 with a bachelor's degree in Psychology.  His dissertation is entitled, "Biculturalism among Indigenous College Students".  His clinical experience includes working at the Counseling Center at BYU, Counseling Services at BYU Hawaii, The Gathering Place in Orem, UT, and the Utah State Hospital. </span></p>
<p style="text-align: left;"><span style="font-family: andale mono,times; font-size: small;">Dr. Miller's clinical work includes working with mood disorders, anxiety disorders, personality disorders, developmental disorders, grief/loss counseling, relationship concerns, addictions, couples counseling and group counseling.  His professional interests have focused on the treatment of anxiety disorders, particularly obsessive-compulsive disorder.  Dr. Miller is a member of both the Association of Mormon Counselors &amp; Psychotherapists and the American Psychological Association.  Additionally, he was part of the Utah Hospital Task Force that provided disaster relief services in Haiti after the earthquake in 2010. </span><span><span style="font-family: andale mono,times; font-size: small;">Dr. Miller enjoys golfing, cycling, and the outdoors.  He is always looking for a good book and/or film recommendation.   He and his wife Caitlin love to travel and experience new cultures.</span><br /></span></p>]]></OptionalItemsText1><OptionalItemsText2 type="dhtml" label="Optional Item Text 2" readonly="false" hidden="false" required="false"><![CDATA[]]></OptionalItemsText2><OptionalItemsText3 type="dhtml" label="Optional Item Text 3" readonly="false" hidden="false" required="false"><![CDATA[]]></OptionalItemsText3><OptionalItemsText4 type="dhtml" label="Optional Item Text 4" readonly="false" hidden="false" required="false"><![CDATA[]]></OptionalItemsText4><OptionalItemsText5 type="dhtml" label="Optional Item Text 5" readonly="false" hidden="false" required="false"><![CDATA[]]></OptionalItemsText5><OptionalItemsText6 type="dhtml" label="Optional Item Text 6" readonly="false" hidden="false" required="false"><![CDATA[]]></OptionalItemsText6></xPower><xPower Type="GroupStart" GroupStart="" Name="SEOGroup" label="SEO Group" readonly="false" hidden="false" required="false" Expanded="false"><BrowserBarTitle type="string" label="Browser Bar Title" readonly="false" hidden="false" require