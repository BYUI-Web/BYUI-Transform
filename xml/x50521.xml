<script type="text/javascript">
$(function() {
	// "Prepare" will be expanded by default, so the arrow should point up
	$("#down-arrow1").attr("id", "up-arrow1");

	// Is each section currently hidden?
	var visible1 = true;  // have "Prepare" expanded by default
	var visible2 = false;
	var visible3 = false;

	// Expands or contracts the corresponding section,
	//    then changes the arrow to point up or down
	function expandToggle(sectionId) {
		var visible;
		var idNumber;

		// Set some section-specific values
		if (sectionId == "#prepare") {
			visible1 = !visible1;  // is the section visible?
			visible = visible1;    // should arrow point up or down?
			idNumber = "1";        // append to the end of string to
			                       //   make a complete section name
		} else if (sectionId == "#participate") {
			visible2 = !visible2;
			visible = visible2;
			idNumber = "2";
		} else {
			visible3 = !visible3;
			visible = visible3;
			idNumber = "3";
		}

		// Make the arrow point up or down
		if (visible) {
			$("#down-arrow" + idNumber).attr("id", "up-arrow" + idNumber);
		} else {
			$("#up-arrow" + idNumber).attr("id", "down-arrow" + idNumber);
		}

		// Expand the section into and out of view
		$(sectionId + "-content").slideToggle(800);
	}

	// Listen for a section to be clicked

	$("#prepare-click").click(function() {
		expandToggle("#prepare");       // pass only the first
		                                //   part of the section name
	});

	$("#participate-click").click(function() {
		expandToggle("#participate");  // pass only the first
		                               //   part of the section name
	});

	$("#ponder-click").click(function() {
		expandToggle("#ponder");      // pass only the first
		                              //   part of the section name
	});
});
</script>
